                  <Insets bottom="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="28.0" />
               </font>
            </Text>
            <TableView fx:id="table" onMouseClicked="#mouseClicked" prefHeight="280.0" prefWidth="530.0" />
            <HBox prefHeight="48.0" prefWidth="533.0" spacing="10.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#back" text="Back" />
                  <Label fx:id="errorLabel" textFill="#ff4343" />
               </children>
            </HBox>
         </children>
      </VBox>
